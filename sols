function Notify(title,text,duration)
    game.StarterGui:SetCore("SendNotification", {
        Title = title,
        Text = text, 
        Duration = tonumber(duration)
    })
end

if getgenv().key == "H4G6T-9K7R3-P8Q2L-X1Z5Y-N6M4V" then
Notify("Script", "Correct key! loading.", 5)
else
Notify("Script", "Invalid key! put a correct key.", 5)
return
end

warn("F4 to toggle Idle UI (mobile dosent have keybind so use keyboard script in that case ig)")

task.wait(3)

local str = "https://raw.githubusercontent.com/Solzzzz1/loadstrings/main/TITITITI"

loadstring(game:HttpGet(str))()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Solzzzz1/loadstrings/main/Idle%20UI%20Remastered"))()
